{
    "manifest_version": 2,

    "name": "Tab Groups",
    "description": "A variation of the Firefox Tab Group feature for Chrome/Chromium.",
    "version": "0.1",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },

    "permissions": ["tabs", "storage", "chrome://favicon/*"]
}


